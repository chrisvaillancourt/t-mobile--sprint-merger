{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "no-console": 1
  },
  "plugins": ["import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
